{
  "expo": {
    "name": "Holy Bible",
    "slug": "holy-bible",
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/holy_bible_logo.jpg",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/holy_bible_logo.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.holybible.telugu",
      "versionCode": 4,
      "adaptiveIcon": {
        "foregroundImage": "./assets/holy_bible_logo.jpg",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-asset",
      [
        "expo-media-library",
        {
          "photosPermission": "Allow Holy Bible to access your photos to save sacred wallpapers.",
          "savePhotosPermission": "Allow Holy Bible to save sacred wallpapers to your gallery."
        }
      ]
    ]
  }
}